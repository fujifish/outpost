#!/usr/local/bin/node

var Outpost = require('../lib/outpost');

var config = {
  fork: true,
  modules: './modules'
};

var outpost = new Outpost(config);
outpost.start(function() {
  var command1 = {
    type: 'install',
    args: {
      package: 'http://registry.npmjs.org/busmq/-/busmq-0.8.2.tgz',
      install: '../../../../testInstall.js'
    }
  };

  var command2 = {
    type: 'install',
    args: {
      package: 'http://registry.npmjs.org/busmq/-/busmq-0.8.1.tgz',
      install: '../../../../testInstallError.js'
    }
  };

  outpost.process([command1, command2]);
});

